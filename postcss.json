{
    "pluginsOptions": {
        "autoprefixer": {"browsers": ["last 2 versions"]},
        "stylelint": {
            "rules": {
                "string-quotes": "double",
                "selector-no-id": true,
                "function-blacklist": "scale",
                "selector-class-pattern": "[a-z]+"
            }
        },
        "postcss-reporter": {"clearMessages": true}
    }
}